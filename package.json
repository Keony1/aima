{
  "name": "aima",
  "version": "0.0.0",
  "description": "Advanced Inventory Management API",
  "scripts": {
    "build": "rimraf ./build && npx tsc",
    "start": "node build/src/main/index.js",
    "dev": "nodemon src/main/index.ts",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:unit": "npm t -- --watch",
    "test:integration": "npm t -- --config ./jest.integration.config.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "compose:dev": "docker compose -f docker-compose.dev.yml -p aima-dev up",
    "compose": "docker compose up"
  },
  "author": "Keony Willian Schr√∂er",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "typeorm": "^0.3.19",
    "typeorm-extension": "^3.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
